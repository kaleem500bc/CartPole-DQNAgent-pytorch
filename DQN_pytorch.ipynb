      "source": [
        "# State space of the environment\n",
        "state_size = env.observation_space.shape[0]\n",
        "\n",
        "# Action space of the agent in the environment\n",
        "action_size = env.action_space.n\n",
        "\n",
        "# Initialze the agent which composes of the model (predict value of the state) and target model\n",
        "agent = Agent(state_size, action_size)"
      ],
      "execution_count": 51,
      "outputs": []
    },
    {
      "cell_type": "code",
